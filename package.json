{
  "name": "cv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:functions": "netlify-lambda build src/functions -c ./webpack.config.js",
    "serve:functions": "netlify-lambda serve src/functions -c ./webpack.config.js"
  },
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "apollo-server-lambda": "^2.8.1",
    "axios": "^0.19.0",
    "citation-js": "^0.4.8",
    "core-js": "^2.6.5",
    "dotenv-webpack": "^1.7.0",
    "graphql": "^14.4.2",
    "graphql-type-json": "^0.3.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mongodb": "^3.3.0-beta2",
    "mongoose": "^5.6.9",
    "netlify-lambda": "^1.5.1",
    "qs": "^6.7.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-country-flag": "^1.2.3",
    "vue-i18n": "^8.12.0",
    "vue-router": "^3.0.3",
    "vuetify": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.9.96",
    "@mdi/js": "^3.8.95",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-pwa": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "graphql-tag": "^2.9.0",
    "prettier": "^1.18.2",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-apollo": "^0.21.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
